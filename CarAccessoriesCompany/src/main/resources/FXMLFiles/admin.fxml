                        <ScrollPane layoutX="50.0" layoutY="120.0" prefHeight="650.0" prefWidth="1500.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="650.0" prefWidth="1500.0" />
                          </content>
                        </ScrollPane>
                        <HBox alignment="CENTER_RIGHT" layoutX="50.0" layoutY="50.0" prefHeight="50.0" prefWidth="1500.0">
                           <children>
                              <Button mnemonicParsing="false" text="add a product">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </AnchorPane>
               </children>
