                  <AnchorPane fx:id="profilePane" prefHeight="820.0" prefWidth="1600.0" styleClass="back" visible="false">
                     <children>
                        <ImageView fitHeight="800.0" fitWidth="1400.0" layoutX="100.0" layoutY="65.0" pickOnBounds="true" preserveRatio="true">
