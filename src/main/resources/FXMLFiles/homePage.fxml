<BorderPane prefHeight="900.0" prefWidth="1601.0" styleClass="HomePageStyle" stylesheets="@../assets/HomePageStyle.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.homePage">
    <top>
        <AnchorPane>
            <children>
                <HBox alignment="CENTER_RIGHT" layoutX="-10.0" layoutY="-2.0" spacing="30.0">
<!--                    <ImageView fitHeight="35.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" styleClass="icon">-->
