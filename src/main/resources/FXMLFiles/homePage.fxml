                    <Button id="returnButton" mnemonicParsing="false" onAction="#onLogout" prefHeight="45.0" prefWidth="40.0" styleClass="transparent-button">
                        <graphic>
                            <ImageView fitHeight="35.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../assets/return.png" />
                                </image>
                            </ImageView>
                        </graphic>
                    </Button>

                    <Button id="userButton" mnemonicParsing="false" onAction="#onEditUserProfile" prefHeight="45.0" prefWidth="40.0" styleClass="transparent-button1">
                        <graphic>
                            <ImageView fitHeight="35.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../assets/user.png" />
                                </image>
                            </ImageView>
                        </graphic>
                    </Button>


                </HBox>


            </children>
        </AnchorPane>
    </top>
    <center>
        <VBox id="homepage1" spacing="10.0" alignment="CENTER_LEFT">
            <children>
                <Label fx:id="label" text="Product Catalog" />
                <TextField fx:id="searchField" promptText="Search For Product You Want" />
                <ComboBox fx:id="categoryComboBox" promptText="Select Category">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Interior" />
                            <String fx:value="Exterior" />
                            <String fx:value="Electronics" />
                        </FXCollections>
                    </items>
                </ComboBox>
                <Button fx:id="button1" onAction="#showProducts" text="Show Products" />
                <TextArea fx:id="productTextArea" editable="false" prefHeight="231.0" prefWidth="763.0" wrapText="true" />
                <DatePicker fx:id="preferredDateField" promptText="preferredDate" />
                <TextField fx:id="carMakeModelField" promptText="Car Make Model" />
                <Button fx:id="buuton2" onAction="#insertInstallationRequest" text="Install Request" />
                <Button fx:id="orderBtt" onAction="#OrdrerReq" text="Order Request" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <stylesheets>
                <URL value="@../assets/style.css" />
                <URL value="@../assets/HomePageStyle.css" />
            </stylesheets>
        </VBox>
    </center>
</BorderPane>

